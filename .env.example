# --- June ---
JUNE_CHAT_URL=https://askjune.ai/app/chat
STORAGE_STATE=./data/storageState.json

# --- Prompts & pacing ---
DAILY_MIN=3
DAILY_MAX=6
MIN_DELAY_MS=45000
MAX_DELAY_MS=120000

# --- IMAP (for auto-login / OTP) ---
IMAP_HOST=imap.yourmail.com
IMAP_PORT=993
IMAP_SECURE=true
IMAP_USER=your@email.com
IMAP_PASS=app_password_or_imap_password
# Optional filters to find the OTP email faster
IMAP_FROM=
IMAP_SUBJECT=verification code
# Regex for OTP code (default: 6 digits)
OTP_REGEX=\\b(\\d{6})\\b
# How long to wait for email (ms)
OTP_TIMEOUT_MS=120000

# --- Logging ---
LOG_POINTS=true
